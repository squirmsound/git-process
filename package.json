{
  "name": "gitflow",
  "version": "0.0.0",
  "description": "Gitflow for npm modules.",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib coverage",
    "test": "cross-env BABEL_ENV=commonjs mocha --require babel-core/register --recursive",
    "test:watch": "npm test -- --watch",
    "test:examples": "node demo/",
    "cover": "cross-env BABEL_ENV=commonjs istanbul cover _mocha -- --require babel-core/register --recursive",
    "lint": "eslint src test",
    "lint:watch": "esw -w test src",
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.disney.com/gitflow.git"
  },
  "keywords": [
    "boilerplate",
    "npm",
    "module",
    "node",
    "javascript"
  ],
  "author": "Adrian Aguirre <adrian.a.aguirre@abc.com>",
